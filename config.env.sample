# Azure VM Details
VM_IP="your.vm.ip.address"
SSH_USER="your_ssh_username"
SSH_KEY="~/.ssh/your_ssh_key"

# Tunnel Configuration  
LOCAL_PORT="8080"
TUNNEL_NAME="Brazil SSH Tunnel"

# Connection Options
SSH_OPTIONS="-4 -C -N -o ServerAliveInterval=30 -o ServerAliveCountMax=3"

